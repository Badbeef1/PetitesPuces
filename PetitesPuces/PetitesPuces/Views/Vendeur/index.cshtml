@{ 
   if (Session["vendeurObj"] == null)
   {
      if (Session["clientObj"] != null)
      {
         Response.Redirect(Url.Action("AccueilClient", "Client"));
      }
      else if (Session["gestionnaireObj"] != null)
      {
         Response.Redirect(Url.Action("AccueilGestionnaire", "Gestionnaire"));
      }
      else
      {
         Response.Redirect(Url.Action("Index", "Internaute"));
      }
   }
}

